a = input()
b = input()
c = input()

array = a.split(" ")
n = list(c)
count = len(n)

if b == "decode":
    for i in range(count):
        pos = int(n[i])
        array_2 = list(reversed(array)) #Cast
        if i == count:
            print(array_2[pos])
        else:
            print(array_2[pos],end="")
else:
    for i in range(count):
        pos = int(n[i])
        if i == count:
            print(array[pos])
        else:
            print(array[pos])
